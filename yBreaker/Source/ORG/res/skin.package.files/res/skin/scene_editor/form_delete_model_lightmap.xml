            <event on_click="delete_cur_scene_model_lightmap_btn_click" />
        </control>
        <control name="delete_all_scene_model_lightmap_btn_click" entity="Button" script="" init="">
            <prop Left="56" Top="128" Width="278" Height="24" Enabled="false" Text="删除所有场景的模型的光贴图" TabStop="true" />
            <event on_click="delete_all_scene_model_lightmap_btn_click_click" />
        </control>
    </control>
</form>
