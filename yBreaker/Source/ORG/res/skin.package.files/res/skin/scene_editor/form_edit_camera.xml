<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_edit_camera" init="main_form_init">
        <prop ShowGrid="true" Left="266" Top="176" Width="398" Height="486" BackColor="150,152,155,255" Font="GE_FONT" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="camera_list" entity="ListBox" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="11" Top="77" Width="142" Height="342" Text="ListBox1" TabStop="true" />
            <event on_select_changed="camera_list_select_changed" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" />
                </subor>
            </subor>
        </control>
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="131" Top="5" Width="70" Height="24" Text="摄像机列表" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="163" Top="45" Width="38" Height="20" Text="名称：" />
        </control>
        <control name="name_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="211" Top="45" Width="174" Height="20" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="GroupBox1" entity="GroupBox" script="" init="">
            <prop Left="163" Top="69" Width="118" Height="110" BackColor="0,255,255,255" Text="位置" />
            <control name="pos_x" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="24" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="pos_changed" on_drag="pos_changed" />
            </control>
            <control name="pos_y" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="48" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="pos_changed" on_drag="pos_changed" />
            </control>
            <control name="pos_z" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="72" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="pos_changed" on_drag="pos_changed" />
            </control>
        </control>
        <control name="add_btn" entity="Button" script="" init="">
            <prop Left="163" Top="181" Width="46" Height="24" Text="添加" TabStop="true" />
            <event on_click="add_btn_click" />
        </control>
        <control name="fix_btn" entity="Button" script="" init="">
            <prop Left="219" Top="181" Width="46" Height="24" Text="修改" TabStop="true" />
            <event on_click="fix_btn_click" />
        </control>
        <control name="del_btn" entity="Button" script="" init="">
            <prop Left="275" Top="181" Width="46" Height="24" Text="删除" TabStop="true" />
            <event on_click="del_btn_click" />
        </control>
        <control name="ok_btn" entity="Button" script="" init="">
            <prop Left="115" Top="453" Width="46" Height="24" Text="确定" TabStop="true" />
            <event on_click="ok_btn_click" />
        </control>
        <control name="cancel_btn" entity="Button" script="" init="">
            <prop Left="243" Top="453" Width="46" Height="24" Text="取消" TabStop="true" />
            <event on_click="cancel_btn_click" />
        </control>
        <control name="up_btn" entity="Button" script="" init="">
            <prop Style="UpArrow" Left="19" Top="45" Width="22" Height="22" TabStop="true" />
            <event on_click="up_btn_click" />
        </control>
        <control name="down_btn" entity="Button" script="" init="">
            <prop Style="DownArrow" Left="51" Top="45" Width="22" Height="22" TabStop="true" />
            <event on_click="down_btn_click" />
        </control>
        <control name="loop_cbn" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="163" Top="213" Width="80" Height="24" Text="循环播放" TabStop="true" />
            <event on_checked_changed="loop_cbn_checked_changed" />
        </control>
        <control name="stop_cbn" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="251" Top="213" Width="80" Height="24" Text="停止播放" TabStop="true" />
            <event on_checked_changed="stop_cbn_checked_changed" />
        </control>
        <control name="GroupBox2" entity="GroupBox" script="" init="">
            <prop Left="275" Top="69" Width="118" Height="110" BackColor="0,255,255,255" Text="方向" />
            <control name="angle_x" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" Loop="true" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="24" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="angle_changed" on_drag="angle_changed" />
            </control>
            <control name="angle_y" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" Loop="true" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="48" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="angle_changed" on_drag="angle_changed" />
            </control>
            <control name="angle_z" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Min="-1000.000000" Max="1000.000000" Loop="true" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="24" Top="72" Width="70" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="angle_changed" on_drag="angle_changed" />
            </control>
        </control>
        <control name="GroupBox3" entity="GroupBox" script="" init="">
            <prop Left="163" Top="237" Width="222" Height="174" BackColor="0,255,255,255" Text="信息" />
            <control name="trans_time" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="96" Top="136" Width="96" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="trans_time_changed" on_drag="trans_time_changed" />
            </control>
            <control name="Label2" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="24" Top="136" Width="70" Height="20" Text="过渡时间：" />
            </control>
            <control name="trans_matter_cbx" entity="ComboBox" script="" init="">
                <prop ButtonWidth="20" DropDownHeight="50" OnlySelect="true" Left="16" Top="24" Width="190" Height="20" />
                <event on_selected="trans_matter_cbx_selected" />
                <subor name="InputEdit">
                    <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Cursor="WIN_IBEAM" />
                </subor>
                <subor name="DropButton">
                    <prop Width="20" Height="20" />
                </subor>
                <subor name="DropListBox">
                    <prop MouseSelect="true" ItemHeight="16" TextOffsetX="2" ScrollSize="17" />
                    <subor name="VScrollBar">
                        <prop BackColor="255,224,224,224" />
                        <subor name="DecButton">
                            <prop Width="17" Height="15" />
                        </subor>
                        <subor name="IncButton">
                            <prop Width="17" Height="15" />
                        </subor>
                        <subor name="TrackButton">
                            <prop Width="17" Height="10" />
                        </subor>
                    </subor>
                </subor>
            </control>
            <control name="max_edit" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="48" Top="56" Width="54" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="range_changed" on_drag="range_changed" />
            </control>
            <control name="min_edit" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="152" Top="56" Width="54" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="range_changed" on_drag="range_changed" />
            </control>
            <control name="Label3" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="16" Top="56" Width="30" Height="20" Text="MAX：" />
            </control>
            <control name="Label4" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="120" Top="56" Width="30" Height="20" Text="MIN：" />
            </control>
        </control>
        <control name="hide_btn" entity="Button" script="" init="">
            <prop Left="179" Top="453" Width="46" Height="24" Text="隐藏" TabStop="true" />
            <event on_click="hide_btn_click" />
        </control>
        <control name="frame_edit" entity="Edit" script="" init="">
            <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Left="91" Top="45" Width="54" Height="20" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
    </control>
</form>
