<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_screen_shot" init="main_form_init">
        <prop Left="45" Top="221" Width="350" Height="462" BackColor="150,152,155,255" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="128" Top="8" Width="94" Height="16" ShadowColor="0,0,0,0" Text="截屏工具" />
        </control>
        <control name="file_path_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="72" Top="48" Width="238" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="file_path_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="8" Top="48" Width="62" Height="16" ShadowColor="0,0,0,0" Text="存放路径：" />
        </control>
        <control name="file_name_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="8" Top="72" Width="62" Height="16" ShadowColor="0,0,0,0" Text="图片名称：" />
        </control>
        <control name="file_name_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="72" Top="72" Width="142" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="screen_shot_btn" entity="Button" script="" init="">
            <prop Left="216" Top="128" Width="86" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="高级截屏" HintText="按照设置的图片尺寸截屏" TabStop="true" />
            <event on_click="screen_shot_btn_click" />
        </control>
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="320" Top="8" Width="24" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="X" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="file_width_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="8" Top="96" Width="62" Height="16" ShadowColor="0,0,0,0" Text="图片宽度：" />
        </control>
        <control name="file_width_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="72" Top="96" Width="78" Height="20" ShadowColor="0,0,0,0" Text="2048" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="file_height_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="152" Top="96" Width="78" Height="16" ShadowColor="0,0,0,0" Text="图片高度：" />
        </control>
        <control name="file_height_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="216" Top="96" Width="78" Height="20" ShadowColor="0,0,0,0" Text="2048" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="browse_btn" entity="Button" script="" init="">
            <prop Left="312" Top="48" Width="30" Height="20" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="..." HintText="选择存放目录" TabStop="true" />
            <event on_click="browse_btn_click" />
        </control>
        <control name="use_device_size_check" entity="CheckButton" script="" init="">
            <prop Checked="true" BoxSize="12" Left="224" Top="72" Width="118" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="使用当前屏幕尺寸" TabStop="true" />
            <event on_checked_changed="use_device_size_checked_changed" />
        </control>
        <control name="common_screen_shot_btn" entity="Button" script="" init="">
            <prop Left="64" Top="128" Width="86" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="普通截屏" HintText="常规截屏" TabStop="true" />
            <event on_click="common_screen_shot_btn_click" />
        </control>
        <control name="orthcamera_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="8" Top="176" Width="126" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="切换为正交摄影机" TabStop="true" />
            <event on_checked_changed="orthcamera_checked_changed" />
        </control>
        <control name="near_plane_fe" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="40" Top="200" Width="70" Height="20" ShadowColor="0,0,0,0" Text="0.1" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="near_plane_fe_changed" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="8" Top="200" Width="38" Height="16" ShadowColor="0,0,0,0" Text="近裁" />
        </control>
        <control name="Label001" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="120" Top="200" Width="38" Height="16" ShadowColor="0,0,0,0" Text="远裁" />
        </control>
        <control name="far_plane_fe" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="10000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="152" Top="200" Width="70" Height="20" ShadowColor="0,0,0,0" Text="2000.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="far_plane_fe_changed" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="232" Top="200" Width="38" Height="16" ShadowColor="0,0,0,0" Text="距离" />
        </control>
        <control name="radius_fe" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="264" Top="200" Width="70" Height="20" ShadowColor="0,0,0,0" Text="1.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="radius_fe_changed" />
        </control>
        <control name="type_check" entity="CheckButton" script="" init="">
            <prop Checked="true" BoxSize="12" Left="168" Top="176" Width="126" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="使用区域截屏方式" TabStop="true" />
            <event on_checked_changed="type_checked_changed" />
        </control>
        <control name="step_gb" entity="GroupBox" script="" init="">
            <prop Top="352" Width="350" Height="102" BackColor="0,255,255,255" ShadowColor="0,0,0,0" />
            <control name="start_pos_x_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="8" Top="16" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="start_pos_y_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="80" Top="16" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="start_pos_z_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="152" Top="16" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="get_start_pos_btn" entity="Button" script="" init="">
                <prop Left="224" Top="16" Width="110" Height="20" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="取当前点为起点" TabStop="true" />
            </control>
            <control name="end_pos_x_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="8" Top="48" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="end_pos_y_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="80" Top="48" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="end_pos_z_edit" entity="Edit" script="" init="">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Left="152" Top="48" Width="70" Height="20" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Enabled="false" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="get_end_pos_btn" entity="Button" script="" init="">
                <prop Left="224" Top="48" Width="110" Height="20" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="取当前点为终点" TabStop="true" />
            </control>
            <control name="Label4" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="16" Top="72" Width="102" Height="16" ShadowColor="0,0,0,0" Text="摄像机平移步长" />
            </control>
            <control name="step_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="152" Top="72" Width="70" Height="20" ShadowColor="0,0,0,0" Text="50" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
        </control>
        <control name="zone_list_gb" entity="GroupBox" script="" init="">
            <prop Top="224" Width="350" Height="134" BackColor="0,255,255,255" ShadowColor="0,0,0,0" />
            <control name="select_zone_btn" entity="Button" script="" init="">
                <prop Left="216" Top="16" Width="118" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="选择区域 &gt;&gt;" TabStop="true" />
                <event on_click="select_zone_btn_click" />
            </control>
            <control name="zone_list" entity="ListBox" script="" init="">
                <prop Sorted="true" ItemHeight="16" TextOffsetX="2" ScrollSize="17" HasHScroll="true" Left="8" Top="8" Width="118" Height="118" BackColor="255,219,219,219" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
                <subor name="HScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="15" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="15" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="10" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="17" Height="10" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
                <subor name="CornerLabel">
                    <prop Solid="true" RefCursor="WIN_HELP" ShadowColor="0,0,0,0" NoFrame="false" />
                </subor>
            </control>
            <control name="bias_z_edit" entity="Edit" script="" init="">
                <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="136" Top="64" Width="70" Height="20" ShadowColor="0,0,0,0" Text="200" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="Label2" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="136" Top="48" Width="126" Height="16" ShadowColor="0,0,0,0" Text="区域到摄像机的距离" />
            </control>
        </control>
        <control name="pos_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Top="232" Width="6" Height="14" ShadowColor="0,0,0,0" Text="|" />
        </control>
    </control>
</form>
