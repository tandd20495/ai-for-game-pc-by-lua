<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_filename" init="main_form_init">
        <prop Left="21" Top="29" Width="504" Height="384" BackColor="255,150,150,255" Font="GE_FONT" />
        <event on_open="main_form_open" />
        <control name="file_list" entity="ListBox" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="256" Top="48" Width="224" Height="200" Text="ListBox1" TabStop="true" />
            <event on_select_changed="file_select_changed" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" />
            </subor>
        </control>
        <control name="path_tree" entity="TreeView" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" LevelWidth="18" ScrollSize="17" Left="24" Top="48" Width="216" Height="200" Text="TreeView1" TabStop="true" />
            <event on_select_changed="path_select_changed" on_expand_changed="path_expand_changed" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" />
            </subor>
        </control>
        <control name="ok_btn" entity="Button" script="" init="">
            <prop Left="152" Top="336" Width="88" Height="30" Text="确定" TabStop="true" />
            <event on_click="ok_btn_click" />
        </control>
        <control name="cancel_btn" entity="Button" script="" init="">
            <prop Left="264" Top="336" Width="88" Height="30" Text="取消" TabStop="true" />
            <event on_click="cancel_btn_click" />
        </control>
        <control name="name_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="104" Top="264" Width="376" Height="24" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="264" Width="80" Height="24" Text="文件名称：" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="296" Width="80" Height="24" Text="文件类型：" />
        </control>
        <control name="type_combo" entity="ComboBox" script="" init="">
            <prop ButtonWidth="20" DropDownHeight="50" OnlySelect="true" Left="104" Top="296" Width="376" Height="24" Text="*.*" />
            <subor name="InputEdit">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Text="*.*" Cursor="WIN_IBEAM" />
            </subor>
            <subor name="DropListBox">
                <prop MouseSelect="true" ItemHeight="16" TextOffsetX="2" ScrollSize="17" />
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" />
                </subor>
            </subor>
        </control>
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="16" Top="8" Width="456" Height="24" Text="选择文件" />
        </control>
    </control>
</form>
