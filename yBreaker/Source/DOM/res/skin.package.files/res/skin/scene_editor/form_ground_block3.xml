<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_ground_block" init="main_form_init">
        <prop Left="101" Top="117" Width="302" Height="486" BackColor="158,152,154,255" ShadowColor="0,0,0,0" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="112" Top="8" Width="80" Height="22" ShadowColor="0,0,0,0" Text="地块编辑" />
        </control>
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="280" Width="22" Height="24" ShadowColor="0,0,0,0" Text="X" HintText="关闭窗口" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="x_mirror_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="24" Top="88" Width="94" Height="22" ShadowColor="0,0,0,0" Text="X方向镜像" TabStop="true" />
        </control>
        <control name="z_mirror_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="120" Top="88" Width="94" Height="22" ShadowColor="0,0,0,0" Text="Z方向镜像" TabStop="true" />
        </control>
        <control name="raise_btn" entity="Button" script="" init="">
            <prop Left="24" Top="120" Width="62" Height="24" ShadowColor="0,0,0,0" Text="升高" TabStop="true" />
            <event on_click="raise_btn_click" />
        </control>
        <control name="sink_btn" entity="Button" script="" init="">
            <prop Left="192" Top="120" Width="62" Height="24" ShadowColor="0,0,0,0" Text="降低" TabStop="true" />
            <event on_click="sink_btn_click" />
        </control>
        <control name="range_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="96" Top="120" Width="86" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="32" Width="270" Height="22" ShadowColor="0,0,0,0" Text="按[C]复制(如果没有选择高度图，则按" />
        </control>
        <control name="save_terrain_check" entity="CheckButton" script="" init="">
            <prop Checked="true" BoxSize="12" Left="16" Top="163" Width="254" Height="22" ShadowColor="0,0,0,0" Text="保存地形(高度图.tga)(必选项)" TabStop="true" />
        </control>
        <control name="save_model_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="192" Width="158" Height="22" ShadowColor="0,0,0,0" Text="保存模型(可选项)" TabStop="true" />
        </control>
        <control name="save_filename_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="104" Top="224" Width="142" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="16" Top="224" Width="86" Height="22" ShadowColor="0,0,0,0" Text="保存文件名:" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="16" Top="144" Width="272" Height="22" ShadowColor="0,0,0,0" Text="------------上下设置不相关------------" />
        </control>
        <control name="file_list" entity="ListBox" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="16" Top="336" Width="126" Height="102" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
            <event on_select_changed="file_select_changed" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" ShadowColor="0,0,0,0" />
                </subor>
            </subor>
        </control>
        <control name="save_btn" entity="Button" script="" init="">
            <prop Left="250" Top="224" Width="46" Height="24" ShadowColor="0,0,0,0" Text="保存" TabStop="true" />
            <event on_click="save_btn_click" />
        </control>
        <control name="sample_pict" entity="Picture" script="" init="">
            <prop CenterX="-1" CenterY="-1" Left="160" Top="336" Width="126" Height="102" ShadowColor="0,0,0,0" />
        </control>
        <control name="Label4" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="16" Top="256" Width="80" Height="22" ShadowColor="0,0,0,0" Text="选择高度图:" />
        </control>
        <control name="Label5" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="56" Width="270" Height="22" ShadowColor="0,0,0,0" Text="鼠标框选地形进行复制)" />
        </control>
        <control name="hide_btn" entity="Button" script="" init="">
            <prop Left="256" Width="22" Height="24" ShadowColor="0,0,0,0" Text="-" HintText="仅隐藏窗口并不销毁窗口，用于调试" TabStop="true" />
            <event on_click="hide_btn_click" />
        </control>
        <control name="lt_btn" entity="Button" script="" init="">
            <prop Icon="ico\undo" Left="152" Top="448" Width="30" Height="30" ShadowColor="0,0,0,0" TabStop="true" />
            <event on_click="lt_btn_click" />
        </control>
        <control name="rt_btn" entity="Button" script="" init="">
            <prop Icon="ico\redo" Left="264" Top="448" Width="30" Height="30" ShadowColor="0,0,0,0" TabStop="true" />
            <event on_click="rt_btn_click" />
        </control>
        <control name="Label6" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="16" Top="448" Width="134" Height="22" ShadowColor="0,0,0,0" Text="旋转新拷贝的场景:" />
        </control>
        <control name="angle_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="192" Top="448" Width="62" Height="20" ShadowColor="0,0,0,0" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_lost_focus="angle_edit_lost_focus" on_drag="angle_edit_drag" />
        </control>
        <control name="hm_path_cbx" entity="ComboBox" script="" init="">
            <prop ButtonWidth="20" DropDownHeight="140" OnlySelect="true" Left="16" Top="312" Width="142" Height="20" ShadowColor="0,0,0,0" />
            <event on_selected="hm_path_cbx_selected" />
            <subor name="InputEdit">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" />
            </subor>
            <subor name="DropButton">
                <prop Width="20" Height="20" ShadowColor="0,0,0,0" />
            </subor>
            <subor name="DropListBox">
                <prop MouseSelect="true" ItemHeight="16" TextOffsetX="2" ScrollSize="17" ShadowColor="0,0,0,0" />
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="17" Height="10" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
            </subor>
        </control>
        <control name="cancel_select_btn" entity="Button" script="" init="">
            <prop Left="168" Top="312" Width="70" Height="24" ShadowColor="0,0,0,0" Text="取消选择" TabStop="true" />
            <event on_click="cancel_select_btn_click" />
        </control>
        <control name="Label7" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="8" Top="72" Width="286" Height="22" ShadowColor="0,0,0,0" Text="--------------------------------------" />
        </control>
        <control name="auto_adjust_model_height_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="104" Top="256" Width="150" Height="22" ShadowColor="0,0,0,0" Text="自动调整模型高度" TabStop="true" />
        </control>
        <control name="heightmap_scale_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" TextOffsetX="2" Caret="Default" Left="128" Top="280" Width="46" Height="22" ShadowColor="0,0,0,0" Text="1.0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="Label8" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="8" Top="280" Width="118" Height="22" ShadowColor="0,0,0,0" Text="高度图缩放比例" />
        </control>
    </control>
</form>
