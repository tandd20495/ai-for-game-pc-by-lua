<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_link_model" init="main_form_init">
        <prop ShowGrid="true" Left="21" Top="21" Width="318" Height="430" ShadowColor="0,0,0,0" Font="GE_FONT" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="280" Top="8" Width="22" Height="22" ShadowColor="0,0,0,0" Text="X" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="120" Top="8" Width="78" Height="22" ShadowColor="0,0,0,0" Text="模型连接" />
        </control>
        <control name="hint_btn" entity="Button" script="" init="">
            <prop Left="256" Top="8" Width="22" Height="22" ShadowColor="0,0,0,0" Text="-" TabStop="true" />
            <event on_click="hint_btn_click" />
        </control>
        <control name="src_gbx" entity="GroupBox" script="" init="">
            <prop Left="8" Top="48" Width="302" Height="142" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="源模型" />
            <control name="src_id_edit" entity="Edit" script="" init="">
                <prop ReadOnly="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="16" Top="56" Width="126" Height="20" BackColor="128,165,165,165" ShadowColor="0,0,0,0" Text="0-0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="src_id_edit_changed" />
            </control>
            <control name="src_get_btn" entity="Button" script="" init="">
                <prop Left="16" Top="24" Width="126" Height="20" ShadowColor="0,0,0,0" Text="获取当前模型" TabStop="true" />
                <event on_click="src_get_btn_click" />
            </control>
            <control name="src_point_list" entity="ListBox" script="" init="">
                <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="144" Top="16" Width="142" Height="110" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="17" Height="10" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
            </control>
        </control>
        <control name="dst_gbx" entity="GroupBox" script="" init="">
            <prop Left="8" Top="192" Width="302" Height="142" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="目标模型" />
            <control name="dst_id_edit" entity="Edit" script="" init="">
                <prop ReadOnly="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="16" Top="56" Width="118" Height="20" BackColor="128,165,165,165" ShadowColor="0,0,0,0" Text="0-0" Cursor="WIN_IBEAM" TabStop="true" />
                <event on_changed="dst_id_edit_changed" />
            </control>
            <control name="dst_get_btn" entity="Button" script="" init="">
                <prop Left="16" Top="24" Width="118" Height="22" ShadowColor="0,0,0,0" Text="获取当前模型" TabStop="true" />
                <event on_click="dst_get_btn_click" />
            </control>
            <control name="dst_point_list" entity="ListBox" script="" init="">
                <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="144" Top="16" Width="142" Height="110" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="17" Height="15" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="17" Height="10" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
            </control>
            <control name="Label2" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="16" Top="104" Width="30" Height="20" ShadowColor="0,0,0,0" Text="旋转" />
            </control>
            <control name="angle_edit" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="1.000000" Min="-360.000000" Max="360.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="64" Top="104" Width="62" Height="20" ShadowColor="0,0,0,0" Text="0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
        </control>
        <control name="link_btn" entity="Button" script="" init="">
            <prop Left="16" Top="336" Width="126" Height="20" ShadowColor="0,0,0,0" Text="连接" TabStop="true" />
            <event on_click="link_btn_click" />
        </control>
        <control name="GroupBox1" entity="GroupBox" script="" init="">
            <prop Left="8" Top="360" Width="302" Height="62" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="自动连接" />
            <control name="auto_link_cbn" entity="CheckButton" script="" init="">
                <prop BoxSize="12" Left="16" Top="24" Width="54" Height="20" ShadowColor="0,0,0,0" Text="开关" TabStop="true" />
            </control>
            <control name="Label3" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="72" Top="24" Width="38" Height="20" ShadowColor="0,0,0,0" Text="距离" />
            </control>
            <control name="dis_link_edit" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="0.100000" Max="10.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="112" Top="24" Width="54" Height="20" ShadowColor="0,0,0,0" Text="1.0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="Label4" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="176" Top="24" Width="38" Height="20" ShadowColor="0,0,0,0" Text="范围" />
            </control>
            <control name="range_link_edit" entity="Float_Edit" script="" init="">
                <prop Format="%.4f" DragStep="1.000000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="224" Top="24" Width="62" Height="20" ShadowColor="0,0,0,0" Text="10.0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
        </control>
    </control>
</form>
