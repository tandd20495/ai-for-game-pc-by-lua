<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_custom_control_panel" init="main_form_init">
        <prop ShowGrid="true" Left="229" Top="101" Width="318" Height="534" ForeColor="255,0,255,0" BackColor="150,152,155,255" ShadowColor="0,0,0,0" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="96" Top="16" Width="118" Height="16" ForeColor="255,0,0,255" ShadowColor="0,0,0,0" Text="自定义设置窗口" />
        </control>
        <control name="change_camera_mode_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="48" Width="254" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="改变摄像机模式（WS不改变高度）" TabStop="true" />
            <event on_checked_changed="change_camera_mode_checked_changed" />
        </control>
        <control name="save_btn" entity="Button" script="" init="">
            <prop NormalImage="skin\png\common\button\button_out.png" FocusImage="skin\png\common\button\button_on.png" PushImage="skin\png\common\button\button_down.png" Left="48" Top="-38" Width="70" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" VAnchor="Bottom" Text="保存" TabStop="true" DrawMode="Expand" />
            <event on_click="save_btn_click" />
        </control>
        <control name="unshow_tree_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="72" Width="94" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="不显示树" TabStop="true" />
            <event on_checked_changed="unshow_tree_checked_changed" />
        </control>
        <control name="show_refill_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="160" Top="72" Width="118" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="显示替代模型" TabStop="true" />
            <event on_checked_changed="show_refill_checked_changed" />
        </control>
        <control name="unshow_fog_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="96" Width="94" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="不显示雾" TabStop="true" />
            <event on_checked_changed="unshow_fog_checked_changed" />
        </control>
        <control name="enable_music_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="120" Width="110" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="开启场景音乐" TabStop="true" />
            <event on_checked_changed="enable_music_checked_changed" />
        </control>
        <control name="enable_sound_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="160" Top="120" Width="110" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="开启场景声音" TabStop="true" />
            <event on_checked_changed="enable_sound_checked_changed" />
        </control>
        <control name="balloon_color_label" entity="Label" script="" init="">
            <prop Solid="true" RefCursor="WIN_HELP" Left="16" Top="152" Width="14" Height="14" ShadowColor="0,0,0,0" NoFrame="false" />
        </control>
        <control name="balloon_color_btn" entity="Button" script="" init="">
            <prop NormalImage="skin\png\common\button\button_out.png" FocusImage="skin\png\common\button\button_on.png" PushImage="skin\png\common\button\button_down.png" Left="32" Top="146" Width="246" Height="30" ForeColor="255,0,160,0" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="改变气球颜色" TabStop="true" DrawMode="Expand" />
            <event on_click="balloon_color_btn_click" />
        </control>
        <control name="cancel_btn" entity="Button" script="" init="">
            <prop NormalImage="skin\png\common\button\button_out.png" FocusImage="skin\png\common\button\button_on.png" PushImage="skin\png\common\button\button_down.png" Left="216" Top="-38" Width="70" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" VAnchor="Bottom" Text="不保存" TabStop="true" DrawMode="Expand" />
            <event on_click="cancel_btn_click" />
        </control>
        <control name="desc_color_label" entity="Label" script="" init="">
            <prop Solid="true" RefCursor="WIN_HELP" Left="16" Top="184" Width="14" Height="16" ShadowColor="0,0,0,0" NoFrame="false" />
        </control>
        <control name="desc_color_btn" entity="Button" script="" init="">
            <prop NormalImage="skin\png\common\button\button_out.png" FocusImage="skin\png\common\button\button_on.png" PushImage="skin\png\common\button\button_down.png" Left="32" Top="176" Width="246" Height="30" ForeColor="255,0,160,0" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="改变描述信息颜色" TabStop="true" DrawMode="Expand" />
            <event on_click="desc_color_btn_click" />
        </control>
        <control name="balloon_far_distance_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="208" Width="198" Height="16" ForeColor="255,0,160,0" ShadowColor="0,0,0,0" Text="场景内气球可见距离（米）" />
        </control>
        <control name="balloon_far_distance_floatedit" entity="Float_Edit" script="" init="">
            <prop Format="%.1f" DragStep="1.000000" Max="1000.000000" OnlyDigit="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="216" Top="208" Width="62" Height="20" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="balloon_far_distance_floatedit_changed" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="-70" Width="262" Height="16" ForeColor="255,0,160,0" ShadowColor="0,0,0,0" VAnchor="Bottom" Text="说明：绿字表示下次打开场景生效" />
        </control>
        <control name="show_sound_range_model_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="256" Width="158" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="显示声音范围模型" TabStop="true" />
            <event on_checked_changed="show_sound_range_model_checked_changed" />
        </control>
        <control name="show_hide_model_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="280" Width="270" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="是否显示具有隐藏属性标签的模型" TabStop="true" />
            <event on_checked_changed="show_hide_model_checked_changed" />
        </control>
        <control name="player_can_climb_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="304" Width="134" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="玩家可以爬墙" TabStop="true" />
            <event on_checked_changed="player_can_climb_checked_changed" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="336" Width="110" Height="16" ForeColor="255,0,0,255" ShadowColor="0,0,0,0" Text="玩家移动速度" />
        </control>
        <control name="player_move_speed_fe" entity="Float_Edit" script="" init="">
            <prop Format="%.1f" DragStep="1.000000" Min="1.000000" Max="40.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="128" Top="336" Width="62" Height="20" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Text="6.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="player_move_speed_fe_changed" />
        </control>
        <control name="lbl_2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="360" Width="80" Height="16" ForeColor="255,0,0,255" ShadowColor="0,0,0,0" Text="NPC显示距离" />
        </control>
        <control name="npc_show_distance" entity="Edit" script="" init="">
            <prop ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="120" Top="360" Width="80" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="npc_show_distance_changed" />
        </control>
        <control name="player_can_effect_logic" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="384" Width="118" Height="24" ForeColor="255,0,0,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="玩家是否影响逻辑" TabStop="true" />
            <event on_checked_changed="player_can_effect_logic_checked_changed" />
        </control>
        <control name="lbl_3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="232" Width="80" Height="16" ForeColor="255,0,0,255" ShadowColor="0,0,0,0" Text="气球高度" />
        </control>
        <control name="ball_height_bar" entity="TrackBar" script="" init="">
            <prop Orient="Horizon" Maximum="100" Left="102" Top="235" Width="134" Height="16" ShadowColor="0,0,0,0" />
            <event on_value_changed="ball_height_bar_value_changed" />
            <subor name="TrackButton">
                <prop Width="12" Height="16" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
            </subor>
        </control>
        <control name="ball_height_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="248" Top="236" Width="54" Height="16" ShadowColor="0,0,0,0" />
        </control>
        <control name="logic_data_btn" entity="Button" script="" init="">
            <prop Left="32" Top="424" Width="254" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="关联场景的逻辑数据" TabStop="true" />
            <event on_click="logic_data_btn_click" />
        </control>
    </control>
</form>
