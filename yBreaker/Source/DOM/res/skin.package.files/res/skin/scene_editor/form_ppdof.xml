<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="scene_editor\form_ppdof" init="main_form_init">
        <prop Left="29" Top="61" Width="398" Height="230" BackColor="150,152,155,255" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="ok_btn" entity="Button" script="" init="">
            <prop Left="80" Top="192" Width="78" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="确定" TabStop="true" />
            <event on_click="ok_btn_click" />
        </control>
        <control name="cancel_btn" entity="Button" script="" init="">
            <prop Left="240" Top="192" Width="78" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="取消" TabStop="true" />
            <event on_click="cancel_btn_click" />
        </control>
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="128" Top="8" Width="166" Height="22" ShadowColor="0,0,0,0" Text="景深参数" />
        </control>
        <control name="ppdof_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="24" Top="48" Width="86" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="开启景深" TabStop="true" />
            <event on_checked_changed="ppdof_checked_changed" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="80" Width="62" Height="16" ShadowColor="0,0,0,0" Text="焦距" />
        </control>
        <control name="focusdepth_floatedit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.010000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="96" Top="80" Width="80" Height="20" ShadowColor="0,0,0,0" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="focusdepth_floatedit_changed" on_drag="focusdepth_floatedit_drag" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="112" Width="62" Height="16" ShadowColor="0,0,0,0" Text="模糊度" />
        </control>
        <control name="blurvalue_floatedit" entity="Float_Edit" script="" init="">
