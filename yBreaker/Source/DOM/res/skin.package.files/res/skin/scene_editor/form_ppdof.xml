            <event on_changed="blurvalue_floatedit_changed" on_drag="blurvalue_floatedit_drag" />
        </control>
        <control name="Label4" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="144" Width="62" Height="16" ShadowColor="0,0,0,0" Text="模糊上限" />
        </control>
        <control name="maxofblur_floatedit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="96" Top="144" Width="80" Height="20" ShadowColor="0,0,0,0" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="maxofblur_floatedit_changed" on_drag="maxofblur_floatedit_drag" />
        </control>
        <control name="linear_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="200" Top="48" Width="86" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="线性景深" TabStop="true" />
            <event on_checked_changed="linear_check_checked_changed" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="200" Top="80" Width="62" Height="16" ShadowColor="0,0,0,0" Text="开始距离" />
        </control>
        <control name="start_depth_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.100000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="272" Top="80" Width="80" Height="20" ShadowColor="0,0,0,0" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
