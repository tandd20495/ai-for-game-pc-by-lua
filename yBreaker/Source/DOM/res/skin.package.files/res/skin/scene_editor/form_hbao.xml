        </control>
        <control name="ao_blur_sharpness_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.100000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="344" Top="200" Width="126" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_drag="ao_blur_sharpness_edit_changed" on_changed="ao_blur_sharpness_edit_changed" />
        </control>
        <control name="ao_half_res_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="32" Top="72" Width="198" Height="22" Text="使用一半屏幕分辨率进行遮挡计算" TabStop="true" />
            <event on_checked_changed="ao_half_res_check_checked_changed" />
        </control>
        <control name="Label12" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="32" Top="200" Width="78" Height="22" Text="遮挡角度偏置" />
        </control>
        <control name="ao_angle_bias_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="1.000000" Max="90.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="112" Top="200" Width="126" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_drag="ao_angle_bias_edit_changed" on_changed="ao_angle_bias_edit_changed" />
        </control>
        <control name="ao_distance_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="1.000000" Max="1000.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="344" Top="152" Width="126" Height="20" Text="0.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_drag="ao_distance_edit_changed" on_changed="ao_distance_edit_changed" />
        </control>
        <control name="Label11" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="264" Top="152" Width="78" Height="22" Text="遮挡有效距离" />
        </control>
    </control>
</form>
