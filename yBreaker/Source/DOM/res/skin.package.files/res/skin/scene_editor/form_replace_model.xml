        <event on_open="main_form_open" />
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="128" Top="16" Width="120" Height="22" ShadowColor="0,0,0,0" Text="批量替换模型" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="32" Top="56" Width="62" Height="22" ShadowColor="0,0,0,0" Text="旧模型：" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="32" Top="88" Width="62" Height="22" ShadowColor="0,0,0,0" Text="新模型：" />
        </control>
        <control name="old_model_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="96" Top="56" Width="198" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="new_model_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="96" Top="88" Width="198" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
