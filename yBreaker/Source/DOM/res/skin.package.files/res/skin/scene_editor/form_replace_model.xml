            <prop Left="304" Top="88" Width="54" Height="24" ShadowColor="0,0,0,0" Text="选择" TabStop="true" />
            <event on_click="select_btn_click" />
        </control>
        <control name="only_replace_select_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="32" Top="120" Width="326" Height="24" ShadowColor="0,0,0,0" Text="仅替换选择的模型，且非指定资源不替换" TabStop="true" />
