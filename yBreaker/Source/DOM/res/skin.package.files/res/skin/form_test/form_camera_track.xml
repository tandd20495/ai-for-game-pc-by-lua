<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_test\form_camera_track" init="main_form_init">
        <prop Left="149" Top="165" Width="894" Height="398" BackColor="161,173,170,255" ShadowColor="0,0,0,0" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_close="on_main_form_close" on_open="main_form_open" />
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="152" Top="8" Width="102" Height="24" ShadowColor="0,0,0,0" Text="摄像机参数" />
        </control>
        <control name="ok_btn" entity="Button" script="" init="">
            <prop Left="104" Top="360" Width="80" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="确定" TabStop="true" />
            <event on_click="ok_btn_click" />
        </control>
        <control name="cancel_btn" entity="Button" script="" init="">
            <prop Left="224" Top="360" Width="80" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="取消" TabStop="true" />
            <event on_click="cancel_btn_click" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="32" Top="40" Width="94" Height="16" ShadowColor="0,0,0,0" Text="摄像机位置：" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="32" Top="72" Width="94" Height="22" ShadowColor="0,0,0,0" Text="摄像机方向：" />
        </control>
        <control name="pos_x_edit" entity="Edit" script="" init="">
            <prop MaxDigit="99999" ChangedEvent="true" MaxLength="5" TextOffsetX="2" Caret="Default" Left="128" Top="40" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="pos_y_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" MaxDigit="99999" ChangedEvent="true" MaxLength="5" TextOffsetX="2" Caret="Default" Left="216" Top="40" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="pos_z_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" MaxDigit="99999" ChangedEvent="true" MaxLength="5" TextOffsetX="2" Caret="Default" Left="304" Top="40" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="dir_x_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" MaxDigit="99995" MaxLength="5" TextOffsetX="2" Caret="Default" Left="128" Top="72" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="dir_y_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" MaxDigit="99999" MaxLength="5" TextOffsetX="2" Caret="Default" Left="216" Top="72" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="dir_z_edit" entity="Edit" script="" init="">
            <prop OnlyDigit="true" MaxDigit="9999" MaxLength="5" TextOffsetX="2" Caret="Default" Left="304" Top="72" Width="78" Height="22" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="btn_resume" entity="Button" script="" init="">
            <prop Left="264" Top="424" Width="110" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="恢复原始设置" TabStop="true" />
            <event on_click="on_btn_resume_click" />
        </control>
        <control name="btn_goto" entity="Button" script="" init="">
            <prop Left="400" Top="72" Width="142" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="跳转" TabStop="true" />
            <event on_click="on_btn_goto_click" />
        </control>
        <control name="mltbox_camera_pos" entity="MultiTextBox" script="" init="">
            <prop TextColor="255,0,0,255" ViewRect="0,0,726,240" LineHeight="19" AutoScroll="true" ScrollSize="17" HasVScroll="true" Solid="true" Left="32" Top="104" Width="726" Height="240" ShadowColor="0,0,0,0" Text="mltbox_camera_pos" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
            </subor>
        </control>
        <control name="btn_start_look" entity="Button" script="" init="">
            <prop Left="760" Top="136" Width="110" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="开始记录" TabStop="true" />
            <event on_click="on_btn_start_look_click" />
        </control>
        <control name="lbl_3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="760" Top="112" Width="110" Height="16" ShadowColor="0,0,0,0" Text="记录摄像机位置：" />
        </control>
        <control name="btn_end_look" entity="Button" script="" init="">
            <prop Left="760" Top="232" Width="110" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="停止记录" TabStop="true" />
            <event on_click="on_btn_end_look_click" />
        </control>
        <control name="btn_clear_look" entity="Button" script="" init="">
            <prop Left="760" Top="264" Width="110" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="清除记录" TabStop="true" />
            <event on_click="on_btn_clear_look_click" />
        </control>
        <control name="btn_updata_pos" entity="Button" script="" init="">
            <prop Left="400" Top="40" Width="142" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="更新坐标" TabStop="true" />
            <event on_click="on_btn_updata_pos_click" />
        </control>
        <control name="cbtn_free_camera" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="552" Top="40" Width="80" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="自由摄像机" TabStop="true" />
            <event on_checked_changed="on_cbtn_free_camera_checked_changed" />
        </control>
        <control name="fipt_freq" entity="Float_Edit" script="" init="">
            <prop Format="%.4f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" ChangedEvent="true" MaxLength="5" TextOffsetX="2" Caret="Default" Left="760" Top="160" Width="46" Height="20" ShadowColor="0,0,0,0" Text="1.0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="lbl_1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="808" Top="161" Width="80" Height="16" ShadowColor="0,0,0,0" Text="记录间隔" />
        </control>
    </control>
</form>
