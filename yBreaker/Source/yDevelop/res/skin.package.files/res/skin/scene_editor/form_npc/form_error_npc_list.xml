<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_npc\form_error_npc_list" init="main_form_init">
        <prop ShowGrid="true" Left="21" Top="29" Width="470" Height="470" ShadowColor="0,0,0,0" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="npc_list" entity="ListBox" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" HasHScroll="true" Left="9" Top="32" Width="454" Height="429" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
            <event on_select_double_click="npc_list_select_double_click" />
            <subor name="HScrollBar">
                <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                <subor name="DecButton">
                    <prop Width="15" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="IncButton">
                    <prop Width="15" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="10" Height="17" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
            </subor>
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
            </subor>
            <subor name="CornerLabel">
                <prop Solid="true" RefCursor="WIN_HELP" ShadowColor="0,0,0,0" NoFrame="false" />
            </subor>
        </control>
        <control name="title" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="9" Top="15" Width="80" Height="16" ShadowColor="0,0,0,0" Text="错误NPC列表" />
        </control>
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="432" Top="8" Width="30" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="X" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="refresh" entity="Button" script="" init="">
            <prop Left="344" Top="8" Width="80" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="刷新" TabStop="true" />
            <event on_click="refresh_click" />
        </control>
    </control>
</form>
