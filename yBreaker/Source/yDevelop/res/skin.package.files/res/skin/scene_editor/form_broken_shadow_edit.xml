<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_broken_shadow_edit" init="main_form_init">
        <prop ShowGrid="true" Left="357" Top="253" Width="262" Height="198" ShadowColor="0,0,0,0" Font="GE_FONT" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="title_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="88" Top="8" Width="86" Height="22" ShadowColor="0,0,0,0" Text="编辑残影" />
        </control>
        <control name="hide_btn" entity="Button" script="" init="">
            <prop Left="200" Top="8" Width="22" Height="22" ShadowColor="0,0,0,0" Text="-" TabStop="true" />
            <event on_click="hide_btn_click" />
        </control>
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="224" Top="8" Width="22" Height="22" ShadowColor="0,0,0,0" Text="X" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="enable_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="40" Width="86" Height="24" ShadowColor="0,0,0,0" Text="开启残影" TabStop="true" />
            <event on_checked_changed="enable_checked_changed" />
        </control>
        <control name="interval_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="64" Width="78" Height="20" ShadowColor="0,0,0,0" Text="间隔时间：" />
        </control>
        <control name="interval_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.3f" DragStep="0.001000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="120" Top="64" Width="80" Height="20" ShadowColor="0,0,0,0" Text="0.2" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="interval_edit_changed" />
        </control>
        <control name="life_time_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="88" Width="86" Height="20" ShadowColor="0,0,0,0" Text="生存时间：" />
        </control>
        <control name="life_time_edit" entity="Float_Edit" script="" init="">
            <prop Format="%.3f" DragStep="0.010000" Max="100.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="120" Top="88" Width="80" Height="20" ShadowColor="0,0,0,0" Text="0.5" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="life_time_edit_changed" />
        </control>
        <control name="count_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="112" Width="86" Height="20" ShadowColor="0,0,0,0" Text="残影数量：" />
        </control>
        <control name="count_edit" entity="Float_Edit" script="" init="">
            <prop Format="%1.0f" DragStep="1.000000" Min="-1.000000" Max="10.000000" OnlyDigit="true" DragEvent="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="120" Top="112" Width="80" Height="20" ShadowColor="0,0,0,0" Text="-1.0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="count_edit_changed" />
        </control>
        <control name="color_gbx" entity="GroupBox" script="" init="">
            <prop Left="8" Top="136" Width="246" Height="62" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="颜色" />
            <control name="color_r" entity="Edit" script="" init="">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Left="16" Top="24" Width="40" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="color_g" entity="Edit" script="" init="">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Left="64" Top="24" Width="40" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="color_b" entity="Edit" script="" init="">
                <prop ReadOnly="true" TextOffsetX="2" Caret="Default" Left="112" Top="24" Width="40" Height="20" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            </control>
            <control name="set_color_btn" entity="Button" script="" init="">
                <prop Left="176" Top="24" Width="54" Height="20" ShadowColor="0,0,0,0" Text="设置" TabStop="true" />
                <event on_click="set_color_btn_click" />
            </control>
        </control>
    </control>
</form>
