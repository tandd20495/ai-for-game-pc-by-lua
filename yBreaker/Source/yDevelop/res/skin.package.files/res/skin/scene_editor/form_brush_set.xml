<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_grass" init="main_form_init">
        <prop Left="157" Top="37" Width="254" Height="270" BackColor="152,154,154,255" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_move="main_form_move" on_open="main_form_open" />
        <control name="GroupBox1" entity="GroupBox" script="" init="">
            <prop Left="8" Top="24" Width="238" Height="46" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="编辑方式" />
            <control name="add_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="24" Top="16" Width="54" Height="22" ShadowColor="0,0,0,0" Text="增加" TabStop="true" />
                <event on_checked_changed="add_checked_changed" />
            </control>
            <control name="erase_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="88" Top="16" Width="54" Height="22" ShadowColor="0,0,0,0" Text="删除" TabStop="true" />
                <event on_checked_changed="erase_checked_changed" />
            </control>
            <control name="erase_all_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="144" Top="16" Width="70" Height="22" ShadowColor="0,0,0,0" Text="删除全部" TabStop="true" />
                <event on_checked_changed="erase_all_checked_changed" />
            </control>
        </control>
        <control name="GroupBox2" entity="GroupBox" script="" init="">
            <prop Left="8" Top="72" Width="238" Height="46" BackColor="0,255,255,255" ShadowColor="0,0,0,0" Text="刷子形状" />
            <control name="rect_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="24" Top="16" Width="54" Height="22" ShadowColor="0,0,0,0" Text="方形" TabStop="true" />
                <event on_checked_changed="rect_checked_changed" />
            </control>
            <control name="circle_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="88" Top="16" Width="54" Height="22" ShadowColor="0,0,0,0" Text="圆形" TabStop="true" />
                <event on_checked_changed="circle_checked_changed" />
            </control>
        </control>
        <control name="model_scale_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="120" Width="80" Height="22" ShadowColor="0,0,0,0" Text="刷子尺寸：" />
        </control>
        <control name="model_scale_track" entity="TrackBar" script="" init="">
            <prop Orient="Horizon" Maximum="10" Left="16" Top="144" Width="222" Height="14" ShadowColor="0,0,0,0" />
            <event on_value_changed="model_scale_value_changed" />
            <subor name="TrackButton">
                <prop Width="12" Height="14" ShadowColor="0,0,0,0" />
            </subor>
        </control>
        <control name="model_amount_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="160" Width="80" Height="22" ShadowColor="0,0,0,0" Text="模型密度：" />
        </control>
        <control name="model_amount_track" entity="TrackBar" script="" init="">
            <prop Orient="Horizon" Maximum="14" Left="16" Top="184" Width="222" Height="14" ShadowColor="0,0,0,0" />
            <event on_value_changed="model_amount_value_changed" />
            <subor name="TrackButton">
                <prop Width="12" Height="14" ShadowColor="0,0,0,0" />
            </subor>
        </control>
        <control name="CheckButton1" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="208" Width="94" Height="24" ShadowColor="0,0,0,0" Text="随机方向" TabStop="true" />
        </control>
        <control name="CheckButton2" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="16" Top="232" Width="110" Height="24" ShadowColor="0,0,0,0" Text="适应地面角度" TabStop="true" />
        </control>
    </control>
</form>
