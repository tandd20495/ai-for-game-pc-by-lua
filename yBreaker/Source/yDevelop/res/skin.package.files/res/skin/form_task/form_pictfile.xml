                    </subor>
                </subor>
            </subor>
        </control>
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="152" Top="8" Width="126" Height="24" ShadowColor="0,0,0,0" Text="选择文件" />
