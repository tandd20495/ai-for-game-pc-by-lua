        <control name="name_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="104" Top="264" Width="334" Height="24" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="264" Width="80" Height="24" ShadowColor="0,0,0,0" Text="文件名称：" />
        </control>
        <control name="Label2" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="24" Top="296" Width="80" Height="24" ShadowColor="0,0,0,0" Text="文件类型：" />
        </control>
