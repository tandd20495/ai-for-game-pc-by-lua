<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_stage_main\form_movie" init="main_form_init">
        <prop ShowGrid="true" Width="800" Height="600" BackColor="0,255,255,255" />
        <event on_open="on_main_form_open" on_close="on_main_form_close" />
        <control name="mltbox_up" entity="MultiTextBox" script="" init="">
            <prop TextColor="255,255,255,255" ViewRect="0,0,800,75" ScrollSize="17" Solid="true" Width="800" Height="75" BackColor="255,0,0,0" Font="FZHKJT30" NoFrame="true" DrawMode="FitWindow" BackImage="gui\loading\scene_change_up.png" />
        </control>
        <control name="mltbox_down" entity="MultiTextBox" script="" init="">
            <prop TextColor="255,255,255,255" Transparent="true" ViewRect="0,0,800,75" ScrollSize="17" Solid="true" Top="525" Width="800" Height="75" BackColor="255,0,0,0" Font="FZHKJT30" NoFrame="true" DrawMode="ExpandH" BackImage="gui\loading\scene_change_down.png" />
        </control>
        <control name="btn_top" entity="Button" script="" init="">
            <prop Left="5" Top="5" Width="800" Height="600" ForeColor="0,0,0,0" BackColor="0,255,255,255" LineColor="0,0,0,0" TabStop="true" NoFrame="true" />
            <event on_click="on_btn_top_click" on_right_click="on_btn_top_click" />
        </control>
    </control>
</form>
