<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="admin_yBreaker\yBreaker_events_gui\yBreaker_form_password" init="on_form_main_init">
        <prop LimitInScreen="true" Left="230" Top="150" Width="230" Height="220" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="gui\common\form_back\bg_form.png" />
        <event on_open="on_main_form_open" on_close="on_main_form_close" />
        <control name="btn_close" entity="Button" script="" init="">
            <prop NormalImage="gui\common\button\btn_close_out.png" FocusImage="gui\common\button\btn_close_on.png" PushImage="gui\common\button\btn_close_down.png" FocusBlendColor="255,255,255,255" PushBlendColor="255,255,255,255" DisableBlendColor="255,255,255,255" NormalColor="0,0,0,0" FocusColor="0,0,0,0" PushColor="0,0,0,0" DisableColor="0,0,0,0" Left="195" Top="7" Width="20" Height="22" ForeColor="255,255,255,255" BackColor="255,192,192,192" ShadowColor="0,255,255,255" Font="font_main" TabStop="true" AutoSize="true" DrawMode="FitWindow" InSound="MouseOn_20" ClickSound="cancel_5" />
            <event on_click="on_btn_close_click" />
        </control>
        <control name="lbl_title" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="50" Top="10" Width="120" Height="20" ForeColor="255,255,255,255" ShadowColor="0,0,0,0" Text="Mã hóa mật khẩu" Font="font_title" />
        </control>
              
        <control name="group_box" entity="GroupBox" script="" init="">
            <prop Left="10" Top="36" Width="205" Height="180" BackColor="0,255,255,255" LineColor="0,0,0,0" ShadowColor="0,0,0,0" />
			<control name="lbl_notice" entity="Label" script="" init="">
            <prop Left="0" Top="0" Width="205" Height="15" Align="Center" Visible="False" RefCursor="WIN_HELP" ForeColor="255,197,184,159" ShadowColor="0,0,0,0" Text="@ui_password_capslock" Font="font_name_guild" BackImage="gui\create\bg_rbtn.png" />
			</control>		
			<control name="ipt_pw2_string" entity="Edit" script="" init="">
                <prop PasswordChar="*" Left="2" Top="5" Width="201" Height="30" ChangedEvent="true" MaxLength="95" TextOffsetX="2" Caret="Default" ForeColor="255,255,255,255" BackColor="0,255,255,255" LineColor="0,0,0,0" BlendColor="0,255,255,255" ShadowColor="0,20,0,0" Font="font_text" Cursor="WIN_IBEAM" TabStop="true" DrawMode="ExpandH" Text="" />
				<event on_get_focus="on_pw2_string_get_focus" on_lost_focus="on_pw2_string_lost_focus" on_changed="on_pw2_string_changed" />
			</control>
		    <control name="lbl_pw2_backgrd" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="0" Top="5" Width="205" Height="30" ShadowColor="0,0,0,0" ForeColor="255,255,255,255" DrawMode="Expand" BackImage="gui\special\battlefield\info.png"/>
            </control>
			
            <control name="btn_pw2_encrypt" entity="Button" script="" init="">
                <prop Left="60" Top="50" Width="100" Height="30" NormalImage="gui\common\button\btn_normal_out.png" FocusImage="gui\common\button\btn_normal_on.png" PushImage="gui\common\button\btn_normal_down.png" DisableImage="gui\common\button\btn_normal_forbid.png" FocusBlendColor="255,255,255,255" PushBlendColor="255,255,255,255" DisableBlendColor="255,255,255,255" NormalColor="0,0,0,0" FocusColor="0,0,0,0" PushColor="0,0,0,0" DisableColor="0,0,0,0" ForeColor="255,255,255,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="Mã Hóa" Font="font_main" TabStop="true" DrawMode="ExpandH" InSound="MouseOn_20" ClickSound="ok_7" />
                <event on_click="on_encode_btn_click" />
            </control>
			
			<control name="ipt_pw2_encrypt" entity="Edit" script="" init="">
                <prop Left="2" Top="90" Width="201" Height="30" ChangedEvent="true" MaxLength="95" TextOffsetX="2" Caret="Default" ForeColor="255,255,255,255" BackColor="0,255,255,255" LineColor="0,0,0,0" BlendColor="0,255,255,255" ShadowColor="0,20,0,0" Font="font_text" Cursor="WIN_IBEAM" TabStop="true" DrawMode="ExpandH" Text="TênNhânVật" />
            </control>			
			<control name="lbl_pw2_encrypt_backgrd" entity="Label" script="" init="">
                <prop RefCursor="WIN_HELP" Left="0" Top="90" Width="205" Height="30" ShadowColor="0,0,0,0" ForeColor="255,255,255,255" DrawMode="Expand" BackImage="gui\special\battlefield\info.png"/>
            </control>

            <control name="btn_pw2_save" entity="Button" script="" init="">
                <prop Left="60" Top="130" Width="100" Height="30" NormalImage="gui\common\button\btn_normal_out.png" FocusImage="gui\common\button\btn_normal_on.png" PushImage="gui\common\button\btn_normal_down.png" DisableImage="gui\common\button\btn_normal_forbid.png" FocusBlendColor="255,255,255,255" PushBlendColor="255,255,255,255" DisableBlendColor="255,255,255,255" NormalColor="0,0,0,0" FocusColor="0,0,0,0" PushColor="0,0,0,0" DisableColor="0,0,0,0" ForeColor="255,255,255,255" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="Lưu Mật Khẩu" Font="font_main" TabStop="true" DrawMode="ExpandH" InSound="MouseOn_20" ClickSound="ok_7" />
                <event on_click="on_save_btn_click" />
            </control>			     

        </control>
    </control>
</form>
