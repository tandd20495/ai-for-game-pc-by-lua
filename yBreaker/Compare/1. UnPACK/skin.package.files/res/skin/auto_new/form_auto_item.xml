<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="auto_new\form_auto_item" init="main_form_init">
        <prop LimitInScreen="true" Left="69" Top="38" Width="270" Height="120" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="gui\common\form_back\bg_form.png" />
