        <control name="lbl_title" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="60" Top="6" Width="150" Height="20" ForeColor="255,255,255,255" ShadowColor="0,0,0,0" Text="Tự Dùng Vật Phẩm" Font="font_title_Mojia" />
        </control>
		
		<control name="lbl_1" entity="Label" script="" init="">
