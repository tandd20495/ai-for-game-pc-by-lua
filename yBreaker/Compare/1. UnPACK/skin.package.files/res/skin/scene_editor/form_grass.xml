<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_grass" init="main_form_init">
        <prop Left="157" Top="37" Width="254" Height="750" BackColor="152,154,154,255" Font="Default" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" on_move="main_form_move" />
        <control name="title_label" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="72" Top="8" Width="110" Height="16" Text="花花草草" />
        </control>
        <control name="close_btn" entity="Button" script="" init="">
            <prop Left="232" Width="24" Height="24" Text="X" HintText="关闭" TabStop="true" />
            <event on_click="close_btn_click" />
        </control>
        <control name="grass_list" entity="ListBox" script="" init="">
            <prop ItemHeight="12" TextOffsetX="2" ScrollSize="17" Left="16" Top="232" Width="110" Height="398" Text="ListBox1" TabStop="true" />
            <event on_select_changed="grass_select_changed" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" />
                </subor>
            </subor>
        </control>
        <control name="GroupBox1" entity="GroupBox" script="" init="">
            <prop Left="8" Top="24" Width="238" Height="46" BackColor="0,255,255,255" Text="编辑方式" />
            <control name="add_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="24" Top="16" Width="54" Height="22" Text="增加" TabStop="true" />
                <event on_checked_changed="add_checked_changed" />
            </control>
            <control name="erase_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="88" Top="16" Width="54" Height="22" Text="删除" TabStop="true" />
                <event on_checked_changed="erase_checked_changed" />
            </control>
            <control name="erase_all_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="144" Top="16" Width="70" Height="22" Text="删除全部" TabStop="true" />
                <event on_checked_changed="erase_all_checked_changed" />
            </control>
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="208" Width="110" Height="22" Text="花草类型：" />
        </control>
        <control name="GroupBox2" entity="GroupBox" script="" init="">
            <prop Left="8" Top="72" Width="238" Height="46" BackColor="0,255,255,255" Text="刷子形状" />
            <control name="rect_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="24" Top="16" Width="54" Height="22" Text="方形" TabStop="true" />
                <event on_checked_changed="rect_checked_changed" />
            </control>
            <control name="circle_check" entity="RadioButton" script="" init="">
                <prop BoxSize="12" Left="88" Top="16" Width="54" Height="22" Text="圆形" TabStop="true" />
                <event on_checked_changed="circle_checked_changed" />
            </control>
        </control>
        <control name="grass_scale_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="120" Width="80" Height="22" Text="刷子尺寸：" />
        </control>
        <control name="grass_scale_track" entity="TrackBar" script="" init="">
            <prop Orient="Horizon" Maximum="10" Left="16" Top="144" Width="222" Height="14" />
            <event on_value_changed="grass_scale_value_changed" />
            <subor name="TrackButton">
                <prop Width="12" Height="14" />
            </subor>
        </control>
        <control name="grass_amount_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="160" Width="80" Height="22" Text="花草密度：" />
        </control>
        <control name="grass_amount_track" entity="TrackBar" script="" init="">
            <prop Orient="Horizon" Maximum="14" Left="16" Top="184" Width="222" Height="14" />
            <event on_value_changed="grass_amount_value_changed" />
            <subor name="TrackButton">
                <prop Width="12" Height="14" />
            </subor>
        </control>
        <control name="grass_pict" entity="Picture" script="" init="">
            <prop CenterX="-1" CenterY="-1" Left="152" Top="232" Width="86" Height="86" />
        </control>
        <control name="grass_tex_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="640" Width="80" Height="22" Text="贴图：" />
        </control>
        <control name="grass_width_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="664" Width="80" Height="22" Text="宽度：" />
        </control>
        <control name="grass_height_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="688" Width="80" Height="22" Text="高度：" />
        </control>
        <control name="grass_pitch_label" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="16" Top="712" Width="80" Height="22" Text="倾斜：" />
        </control>
    </control>
</form>
