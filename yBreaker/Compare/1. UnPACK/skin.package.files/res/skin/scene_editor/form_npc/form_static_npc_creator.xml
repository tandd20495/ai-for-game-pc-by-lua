<?xml version="1.0" encoding="utf-8" ?>
<form>
    <control name="main_form" entity="Form" script="form_npc\form_static_npc_creator" init="main_form_init">
        <prop ShowGrid="true" Left="221" Top="37" Width="294" Height="606" BackColor="150,152,155,255" ShadowColor="0,0,0,0" Font="Default" DrawMode="Expand" BackImage="skin\png\form.png" />
        <event on_open="main_form_open" />
        <control name="Label6" entity="Label" script="" init="">
            <prop Align="Center" RefCursor="WIN_HELP" Left="80" Top="8" Width="134" Height="22" ShadowColor="0,0,0,0" Text="NPC配置窗口" />
        </control>
        <control name="put_npc_to_show_err_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="8" Top="32" Width="118" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="提示错误信息" TabStop="true" />
            <event on_checked_changed="put_npc_to_show_err_check_checked_changed" />
        </control>
        <control name="show_spring_range_check" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="136" Top="32" Width="150" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="显示SpringRange" TabStop="true" />
            <event on_checked_changed="show_spring_range_checked_changed" />
        </control>
        <control name="show_npc_cbtn" entity="CheckButton" script="" init="">
            <prop BoxSize="12" Left="8" Top="56" Width="86" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="显示NPC" TabStop="true" />
            <event on_checked_changed="show_npc_cbtn_checked_changed" />
        </control>
        <control name="npc_list" entity="ListBox" script="" init="">
            <prop ItemHeight="16" TextOffsetX="2" ScrollSize="17" Left="8" Top="216" Width="278" Height="382" ShadowColor="0,0,0,0" Text="ListBox1" TabStop="true" />
            <event on_select_click="npc_list_select_click" on_select_double_click="npc_list_select_double_click" />
            <subor name="VScrollBar">
                <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                <subor name="DecButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="IncButton">
                    <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
                <subor name="TrackButton">
                    <prop Width="17" Height="10" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                </subor>
            </subor>
        </control>
        <control name="Label1" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="136" Top="56" Width="70" Height="16" ShadowColor="0,0,0,0" Text="显示距离" />
        </control>
        <control name="npc_dis_edit" entity="Edit" script="" init="">
            <prop TextOffsetX="2" Caret="Default" Left="216" Top="56" Width="62" Height="20" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Text="10" Cursor="WIN_IBEAM" TabStop="true" />
        </control>
        <control name="delete_select_npc_btn" entity="Button" script="" init="">
            <prop Left="208" Top="192" Width="78" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="删除" TabStop="true" />
            <event on_click="delete_select_npc_btn_click" />
        </control>
        <control name="default_rbtn" entity="RadioButton" script="" init="">
            <prop BoxSize="12" Left="8" Top="80" Width="86" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="默认排序" TabStop="true" />
            <event on_checked_changed="sort_rbtn_checked_changed" />
        </control>
        <control name="name_rbtn" entity="RadioButton" script="" init="">
            <prop BoxSize="12" Left="136" Top="80" Width="102" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="按名称排序" TabStop="true" />
            <event on_checked_changed="sort_rbtn_checked_changed" />
        </control>
        <control name="range_rbtn" entity="RadioButton" script="" init="">
            <prop BoxSize="12" Left="8" Top="104" Width="102" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="按区域排序" TabStop="true" />
            <event on_checked_changed="sort_rbtn_checked_changed" />
        </control>
        <control name="from_left" entity="Edit" script="" init="">
            <prop OnlyDigit="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="8" Top="168" Width="62" Height="22" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="sort_range_changed" />
        </control>
        <control name="from_top" entity="Edit" script="" init="">
            <prop OnlyDigit="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="72" Top="168" Width="62" Height="22" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="sort_range_changed" />
        </control>
        <control name="to_left" entity="Edit" script="" init="">
            <prop OnlyDigit="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="144" Top="168" Width="62" Height="22" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="sort_range_changed" />
        </control>
        <control name="to_top" entity="Edit" script="" init="">
            <prop OnlyDigit="true" ChangedEvent="true" TextOffsetX="2" Caret="Default" Left="208" Top="168" Width="62" Height="22" LineColor="255,0,0,255" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" TabStop="true" />
            <event on_changed="sort_range_changed" />
        </control>
        <control name="Label3" entity="Label" script="" init="">
            <prop RefCursor="WIN_HELP" Left="136" Top="168" Width="14" Height="22" ShadowColor="0,0,0,0" Text=":" />
        </control>
        <control name="npcfile_cbox" entity="ComboBox" script="" init="">
            <prop ButtonWidth="20" DropDownHeight="200" Left="8" Top="192" Width="198" Height="22" ShadowColor="0,0,0,0" />
            <event on_selected="npcfile_cbox_selected" />
            <subor name="InputEdit">
                <prop TextOffsetX="2" Caret="Default" ShadowColor="0,0,0,0" Cursor="WIN_IBEAM" />
            </subor>
            <subor name="DropButton">
                <prop Width="20" Height="22" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
            </subor>
            <subor name="DropListBox">
                <prop MouseSelect="true" ItemHeight="16" TextOffsetX="2" ScrollSize="17" ShadowColor="0,0,0,0" />
                <subor name="VScrollBar">
                    <prop BackColor="255,224,224,224" ShadowColor="0,0,0,0" />
                    <subor name="DecButton">
                        <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="IncButton">
                        <prop Width="17" Height="15" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                    <subor name="TrackButton">
                        <prop Width="17" Height="10" BackColor="255,192,192,192" ShadowColor="0,0,0,0" />
                    </subor>
                </subor>
            </subor>
        </control>
        <control name="reload_btn" entity="Button" script="" init="">
            <prop Left="136" Top="112" Width="150" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="重新加载静态NPC高度" TabStop="true" />
            <event on_click="reload_btn_click" />
        </control>
        <control name="save_npc_area" entity="Button" script="" init="">
            <prop Left="136" Top="136" Width="150" Height="24" BackColor="255,192,192,192" ShadowColor="0,0,0,0" Text="保存NPC对应区域" TabStop="true" />
            <event on_click="save_npc_area_click" />
        </control>
    </control>
</form>
